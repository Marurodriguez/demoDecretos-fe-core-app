\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('debito',item)\">\r\n                                    </money-input>\r\n                                    \r\n                                </td>\r\n                                <td>\r\n                                    <money-input *ngIf=\"getMonto('credito',item) != 0\" [formatMoney]=\"formatMoney\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('credito',item)\">\r\n                                    </money-input>\r\n                                </td>\r\n                                <td>\r\n                                    <money-input [formatMoney]=\"formatMoney\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('saldo',item)\">\r\n                                    </money-input>\r\n                                </td>\r\n                            </tr>\r\n                        </ng-template> <!-- let registros -->\r\n                        <tr class=\"tr-total\" *ngIf=\"entidad.registros.length > 1\">\r\n                            <td colspan=\"4\">TOTAL {{entidad.entidadNombre}}</td>\r\n                            <td>\r\n                                <money-input *ngIf=\"getMonto('debito',entidad) != 0\" [formatMoney]=\"formatMoney\" \r\n                                    [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                    [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('debito',entidad)\">\r\n                                </money-input>\r\n                                \r\n                            </td>\r\n                            <td>\r\n                                <money-input *ngIf=\"getMonto('credito',entidad) != 0\" [formatMoney]=\"formatMoney\" \r\n                                    [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                    [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('credito',entidad)\">\r\n                                </money-input>\r\n                            </td>\r\n                            <td>\r\n                                <money-input [formatMoney]=\"formatMoney\" \r\n                                    [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                    [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('saldo',entidad)\">\r\n                                </money-input>\r\n                            </td>\r\n                        </tr>\r\n                    </ng-template> <!-- let entidad -->\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    </ng-template>\r\n</section>\r\n","import { AbstractModel } from \"./Abstract.model\";\r\nimport { ListadoxTerceroModel } from \"./ListadoxTercero.model\";\r\nimport { MonedaModel } from \"./Moneda.model\";\r\n\r\n\r\n/**\r\n * Se usa para agrupar\r\n */\r\nexport class ListadoxTerceroVirtualModel extends AbstractModel {\r\n  public entidadNombre: string = \"\";\r\n  public entidadId: number = 0;\r\n  public entidadCodigo: string = \"\";\r\n  public debitoMonedaPrimaria: number = 0;\r\n  public creditoMonedaPrimaria: number = 0;\r\n  public debitoMonedaSecundaria: number = 0;\r\n  public creditoMonedaSecundaria: number = 0;\r\n\r\n \r\n  // Totales\r\n  public registros:ListadoxTerceroModel[] = [];\r\n  public vExpand: boolean = true; \r\n  constructor(){\r\n    super();\r\n  }\r\n  \r\n}\r\n\r\n","import { ChangeDetectorRef, Component, ViewChild } from '@angular/core';\r\nimport { ActivatedRoute, R