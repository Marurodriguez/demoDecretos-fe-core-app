    tipoVistaChange(TipoVistaSelected) {
        switch (TipoVistaSelected.id) {
            case 0:
                this.tipoVista = TipoVistaEnum.original;
                this.monedaVista = this.empresa.monedaPrimaria;
                break;
            case 1:
                this.tipoVista = TipoVistaEnum.principal;
                this.monedaVista = this.empresa.monedaPrimaria;
                break;
            case 2:
                this.tipoVista = TipoVistaEnum.secundaria;
                this.monedaVista = this.empresa.monedaSecundaria;
                break;
        }
        this.cd.markForCheck();
    }
    /**
     * Muestra el resumen del monto de la moneda secundaria
     * para saber como se transformo, con la tasa..
     * @param reg
     */
    getMonedaAclaracion(reg) {
        let monto = 0;
        let tasa = reg.tasaMoneda;
        let str = "";
        switch (this.tipoVista) {
            case TipoVistaEnum.original:
                // Si se tiene que mostrar según la moneda que se ingreso, 
                // entonces si la moneda ingresada es la secundaría se pone la aclaración
                if (reg.cuenta.moneda.id == this.empresa.monedaSecundaria.id) {
                    monto = Number(reg.debitoMonedaSecundaria + reg.creditoMonedaSecundaria);
                    // Moneda secundaria se muestra con la tasa para la conversión
                    str = this.functionService.formatMoney(monto, reg.cuenta.moneda.codigo);
                    str = str + " x " + this.functionService.formatMoney(tasa, " ");
                    str = "(" + str + ") = " + this.functionService.formatMoney(tasa * monto, this.empresa.monedaPrimaria.codigo);
                }
                break;
            case TipoVistaEnum.principal:
                // Si se tiene que mostrar siempre la moneda principal, entonces
                // Si es una moneda secundaria se debe poner la aclaración
                if (reg.cuenta.moneda.id == this.empresa.monedaSecundaria.id) {
                    monto = Number(reg.debitoMonedaSecundaria + reg.creditoMonedaSecundaria);
                    // Moneda secundaria se muestra con la tasa para la conversión
                    str = this.functionService.formatMoney(monto, reg.cuenta.moneda.codigo);
                    str = str + " x " + this.functionService.formatMoney(tasa, " ");
                    str = "(" + str + ") = " + this.functionService.formatMoney(tasa * monto, this.empresa.monedaPrimaria.codigo);
                }
                break;
            case TipoVistaEnum.secundaria:
                // Si se debe mostrar en moneda secundaría entonces, si es una moneda
                // Principal se debe mostrar la aclaración
                if (reg.cuenta.moneda.id == this.empresa.monedaPrimaria.id) {
                    monto = (reg.debitoMonedaPrimaria + reg.creditoMonedaPrimaria);
                    str = this.functionService.formatMoney(monto, reg.cuenta.moneda.codigo);
                    str = str + " / " + this.functionService.formatMoney(tasa, " ");
                    str = "(" + str + ") = " + this.functionService.formatMoney(monto / tasa, this.empresa.monedaSecundaria.codigo);
                }
                break;
        }
        // if(reg.cuenta.moneda.id == this.monedaVista.id){
        //   monto = Number(reg.debitoMonedaPrimaria + reg.creditoMonedaPrimaria);
        //   if(this.tipoVista == TipoVistaEnum.secundaria){
        //     str = this.functionService.formatMoney(monto,reg.cuenta.moneda.codigo);
        //     str = str + " / " + this.functionService.formatMoney(tasa," ");
        //     str = "(" + str + ") = "  + this.functionService.formatMoney(monto/tasa,this.empresa.monedaPrimaria.codigo);
        //   }
        // }else{
        //   monto = Number(reg.debitoMonedaSecundaria + reg.creditoMonedaSecundaria);
        //   // Moneda secundaria se muestra con la tasa para la conversión
        //   str = this.functionService.formatMoney(monto,reg.cuenta.moneda.codigo);
        //   str = str + " x " + this.functionService.formatMoney(tasa," ");
        //   str = "(" + str + ") = "  + this.functionService.formatMoney(tasa*monto,this.empresa.monedaPrimaria.codigo);
        // }
        return str;
    }
    getMonedaSegunVista(reg) {
        switch (this.tipoVista) {
            case TipoVistaEnum.original:
                return reg.cuenta.moneda;
            case TipoVistaEnum.principal:
                return this.empresa.monedaPrimaria;
                break;
            case TipoVistaEnum.secundaria:
                return this.empresa.monedaSecundaria;
                break;
        }
        return reg.cuenta.moneda;
    }
    /**
     *
     * @param reg Puede ser AsientoRegistro o Asiento
     * @param getDebito Si es true devuelve el debito, y si es false devuelve el credito
     * @returns el total en la moneda expresada
     */
    getTotalSegunMoneda(reg, getDebito) {
        let tipoVistaReg = this.tipoVista;
        if (tipoVistaReg == TipoVistaEnum.original) {
            if (reg.cuenta == undefined) { //Se trata de un asiento principal
                tipoVistaReg = TipoVistaEnum.principal;
            }
            else {
                if (reg.cuenta.moneda.id == this.empresa.monedaPrimaria.id) {
                    tipoVistaReg = TipoVistaEnum.principal;
                }
                else {
                    tipoVistaReg = TipoVistaEnum.secundaria;
                }
            }
        }
        switch (tipoVistaReg) {
            case TipoVistaEnum.principal:
                if (getDebito == true) {
                    return reg.debitoMonedaPrimaria;
                }
                else {
                    return reg.creditoMonedaPrimaria;
                }
                break;
            case TipoVistaEnum.secundaria:
                if (getDebito == true) {
                    return reg.debitoMonedaSecundaria;
                }
                else {
                    return reg.creditoMonedaSecundaria;
                }
                break;
            default:
                return -1;
                break;
        }
    }
    getField(fieldName, row) {
        switch (fieldName) {
            case "fecha":
                let date = this.functionService.getDateFromString(row.fecha, "-");
                return this.functionService.formatDate(date);
                break;
            case "cantidadRegistros":
                if (row.registros && (row === null || row === void 0 ? void 0 : row.registros.length) > 0) {
                    return row.registros.length;
                }
                else {
                    return 0;
                }
                break;
            case "operacion":
                if (row.tipoOperacion) {
                    return row.tipoOperacion.nombre;
                }
                break;
            case "numero-operacion":
                return row.numeroOperacion;
                break;
        }
    }
    getFieldReg(fieldName, row) {
        try {
            switch (fieldName) {
                case "negocio":
                    if (row.negocio && row.negocio.nombre) {
                        return row.negocio.nombre;
                    }
                    break;
                case "entidad":
                    if (row.entidad && row.entidad.nombre) {
                        return row.entidad.nombre;
                    }
                    break;
            }
        }
        catch (ex) {
            return "";
        }
        return "";
    }
    getTotal(tipo) {
        let sumDebito = 0;
        let sumcredito = 0;
        for (let asiento of this.asientos) {
            for (let reg of asiento.registros) {
                sumDebito = sumDebito + reg.debitoMonedaPrimaria;
                sumcredito = sumcredito + reg.creditoMonedaPrimaria;
            }
        }
        if (tipo == 'debito') {
            return sumDebito;
        }
        else {
            return sumcredito;
        }
    }
    /************************************************************************************************************************
     *
     * Eventos
     *
     ************************************************************************************************************************/
    ngOnInit() {
        this.refresh(true);
    }
    /************************************************************************************************************************
     * FUNCTIONS
     *
     ************************************************************************************************************************/
    refresh(force) {
        if (!(this.pageAnt != this.page || this.sizeAnt != this.size || (this.fechaAnt.year != this.fechaAnt.year || this.fecha.month != this.fechaAnt.month || this.fecha.day != this.fechaAnt.day)) && force == false) {
            return; //No hubo cambios!
        }
        this.pageAnt = this.page;
        this.fechaAnt.day = this.fecha.day;
        this.fechaAnt.month = this.fecha.month;
        this.fechaAnt.year = this.fecha.year;
        let paginateListModel = new _coreApp_src_app_models_PaginateList_model__WEBPACK_IMPORTED_MODULE_5__["PaginateListModel"](this.page, this.size);
        paginateListModel.addParameter("empresa", this.userService.getIdEmpresaSelected().toString());
        //FechaInit! --> La fecha seleccionada
        let fechaInit = this.fecha.year + '-' + this.fecha.month + '-' + this.fecha.day + ' 00:00:01';
        let fechaFin = this.fechaFin.year + '-' + this.fechaFin.month + '-' + this.fechaFin.day + ' 23:59:59';
        paginateListModel.addParameter("fechaInicio", fechaInit);
        paginateListModel.addParameter("fechaFin", fechaFin);
        this.dataService.httpFunction(_navigation_ws_ws_routes_config__WEBPACK_IMPORTED_MODULE_10__["enumWS"].ASIENTO_PAGINATE_DTO, this, paginateListModel);
        this.loading = true;
    }
    getClass(reg) {
        try {
            if (reg.debito > 0) {
                return "reg-debe";
            }
            else {
                return "reg-haber";
            }
        }
        catch (ex) {
            return "";
        }
    }
    SetPage(paginate) {
        this.page = paginate.page;
        this.size = paginate.size;
        this.refresh(false);
    }
    /************************************************************************************************************************
     *
     * RESPONSE OK/ERROR
     *
     ************************************************************************************************************************/
    responseOk(httpOperation, http, data, ws) {
        switch (ws.enumUrl) {
            case _navigation_ws_ws_routes_config__WEBPACK_IMPORTED_MODULE_10__["enumWS"].ASIENTO_PAGINATE_DTO:
                this.paginateData = data;
                this.asientos = data.content;
                this.loaded = true;
                this.cd.markForCheck();
                break;
        }
    }
    responseError(urlResource, httpOperation, data, ws) {
        switch (ws.enumUrl) {
            case _navigation_ws_ws_routes_config__WEBPACK_IMPORTED_MODULE_10__["enumWS"].ASIENTO_PAGINATE_DTO:
                alert("error en el paginado");
                break;
        }
    }
