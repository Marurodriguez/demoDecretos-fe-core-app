plate ngFor let-item [ngForOf]=\"entidad.registros\">\r\n                            <tr>\r\n                                <td>\r\n                                    {{getField('tercero',item)}}\r\n                                </td>\r\n                                <td>\r\n                                    {{getField('fecha',item)}}\r\n                                </td>\r\n                                <td>\r\n                                    {{getField('obs',item)}}\r\n                                </td>\r\n                                <td>\r\n                                    {{getField('referencia',item)}}\r\n                                </td>\r\n                                <td>\r\n                                    <money-input *ngIf=\"getMonto('debito',item) != 0\" [formatMoney]=\"formatMoney\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('debito',item)\">\r\n                                    </money-input>\r\n                                    \r\n                                </td>\r\n                                <td>\r\n                                    <money-input *ngIf=\"getMonto('credito',item) != 0\" [formatMoney]=\"formatMoney\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto('credito',item)\">\r\n                                    </money-input>\r\n                                </td>\r\n                                <td>\r\n                                    <money-input [formatMoney]=\"formatMoney\" \r\n                                        [cantDecimales]=\"cantDecimales\" [simple]=\"moneySimple\" \r\n                                        [monedaPrimary]=\"monedaVista\" [initValue]=\"getMonto