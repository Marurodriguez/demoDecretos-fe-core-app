t.data;\r\n    this.titleBarService.title = \"Listado Cuenta x Tercero\";\r\n    this.titleBarService.subTitle = \"Desde esta pantalla se pueden ver los movimientos por tercero y cuenta\";\r\n\r\n    //HARDCODE\r\n    this.fechaInicio= new NgbDate(2000,1,1);\r\n    this.fechaFin = new NgbDate(2023,12,31);\r\n    // this.cuentaSelected.id = 959; //HARDCODE\r\n    // this.refresh(true);\r\n\r\n   }\r\n   public selectCuenta(cuenta: CuentaContableModel) {\r\n    this.cuentaSelected = cuenta; \r\n  }\r\n\r\n   tipoVistaChange(TipoVistaSelected:any){\r\n    switch(TipoVistaSelected.id){\r\n      case TipoVistaEnum.principal: \r\n        this.tipoVista = TipoVistaEnum.principal;\r\n        this.monedaVista = this.empresa.monedaPrimaria;\r\n      break;\r\n      case TipoVistaEnum.secundaria:\r\n        this.tipoVista = TipoVistaEnum.secundaria;\r\n        this.monedaVista = this.empresa.monedaSecundaria;\r\n      break;\r\n    }\r\n    // this.refresh(true);\r\n    //this.cd.markForCheck();\r\n   }\r\n\r\n\r\n  /************************************************************************************************************************\r\n   * Funciones\r\n   *\r\n   ************************************************************************************************************************/\r\n   getStyle(item: any):string {\r\n    try {\r\n        let tab: number = 25;\r\n        let regla: string = \"\";\r\n        tab = tab * item.nivel;\r\n        regla = \"text-indent: \" + tab.toString() + \"px\"; \r\n        return regla;\r\n    }catch(ex){\r\n      return \"\";\r\n    }\r\n    return \"\";\r\n   }\r\n\r\n   getMonto(fieldName: string, item: any): number{\r\n    if(item == undefined || item == null){\r\n      return 0;\r\n    }\r\n    try {\r\n      switch(fieldName){\r\n        case \"debito\":\r\n          if(this.monedaVista.id == this.empresa.monedaPrimaria.id){\r\n            return item.debitoMonedaPrimaria;\r\n          }else{\r\n            return item.debitoMonedaSecundaria;\r\n          }\r\n        break;\r\n        case \"credito\":\r\n          if(this.monedaVista.id == this.empresa.monedaPrimaria.id){\r\n            return item.creditoMonedaPrimaria;\r\n          }else{\r\n            return item.creditoMonedaSecundaria;\r\n          }\r\n        break;\r\n        case \"saldo\":\r\n          if(this.monedaVista.id == this.empresa.monedaPrimaria.id){\r\n            return (item.debitoMonedaPrimaria - item.creditoMonedaPrimaria);\r\n          }else{\r\n            return (item.debitoMonedaSecundaria - item.creditoMonedaSecundaria);\r\n          }\r\n        break;\r\n      }\r\n    }catch(ex){\r\n      return 0;\r\n    }\r\n    return 0;\r\n   }\r\n\r\n   getField(fieldName: string,item: any): string {\r\n    try {\r\n      switch(fieldName){\r\n        case \"tercero\":\r\n          return item.entidadNombre;\r\n        break;\r\n        case \"fecha\":\r\n          return item.fecha;\r\n        break;\r\n        case \"obs\":\r\n          return item.descripcion;\r\n        break;\r\n        case \"referencia\":\r\n          return item.referencia;\r\n        break;\r\n        case \"debito\":\r\n          return item.debitoMonedaPrimaria;\r\n        break;\r\n        case \"credito\":\r\n          return item.creditoMonedaPrimaria;\r\n        break;\r\n        case \"saldo\":\r\n          return (item.debitoMonedaPrimaria - item.creditoMonedaPrimaria).toString();\r\n        break;\r\n      }\r\n    }catch(ex){\r\n      return \"\";\r\n    }\r\n    return \"\";\r\n   }\r\n\r\n   refresh(force:boolean){\r\n    let request: PaginateListModel = new PaginateListModel(0,100000);\r\n    let fechaInicio: string = this.fechaInicio.year + \"-\" + this.fechaInicio.month + \"-\" + this.fechaInicio.day;\r\n    let fechaFin: string = this.fechaFin.year + \"-\" + this.fechaFin.month + \"-\" + this.fechaFin.day;\r\n    \r\n    request.addParameter(\"empresa\",this.empresa.id.toString());\r\n    request.addParameter(\"cuenta\", this.cuentaSelected.id.toString());\r\n    request.addParameter(\"fechaInicio\", fechaInicio);\r\n    request.addParameter(\"fechaFin\", fechaFin);\r\n    this.dataService.httpFunction(enumWS.BALANCE_MOVIMIENTO_CUENTA_TERCERO,this,request,{});\r\n    this.loaded = false;\r\n    \r\n    return true;\r\n   }\r\n  \r\n\r\n   procesarData(data): boolean {\r\n    \r\n    let vEntidad: ListadoxTerceroVirtualModel = new ListadoxTerceroVirtualModel();\r\n    let entidadIdAnt: number = 0;\r\n    this.registros = [];\r\n    let regs: ListadoxTerceroModel[] = [];\r\n    regs = data.content;\r\n\r\n    for(let reg of regs){\r\n      if(reg.entidadId != entidadIdAnt){\r\n        if(entidadIdAnt != 0){\r\n          this.registros.push(vEntidad);\r\n        }\r\n        vEntidad = new ListadoxTerceroVirtualModel(); \r\n        vEntidad.entidadId = reg.entidadId;\r\n        vEntidad.entidadCodigo = reg.entidadCodigo;\r\n        vEntidad.entidadNombre = reg.entidadNombre;\r\n        vEntidad.registros = [];\r\n      }\r\n\r\n      let regCuentaxTercero: ListadoxTerceroModel = reg;\r\n      vEntidad.registros.push(regCuentaxTercero);\r\n      vEntidad.debitoMonedaPrimaria+=regCuentaxTercero.debitoMonedaPrimaria;\r\n      vEntidad.creditoMonedaPrimaria+=regCuentaxTercero.creditoMonedaPrimaria;\r\n      vEntidad.debitoMonedaSecundaria+=regCuentaxTercero.debitoMonedaSecundaria;\r\n      vEntidad.creditoMonedaSecundaria+=regCuentaxTercero.creditoMonedaSecundaria;\r\n      \r\n      entidadIdAnt = regCuentaxTercero.entidadId;\r\n\r\n    }\r\n    // if(regs.length > 0){\r\n    //   vEntidad.registros.push(regs[regs.length-1]);\r\n    // }}\r\n  \r\n    this.cd.markForCheck();\r\n    this.loaded = true;\r\n    return true;\r\n   }\r\n\r\n   public exportar(): boolean {\r\n    this.formatMoney = false;\r\n    this.cd.markForCheck();\r\n    this.exportTableToExcel(\"tableData\");\r\n    //this.formatMoney =true;\r\n    return true;\r\n   }\r\n\r\n\r\n   exportTableToExcel(tableId: string, name?: string) {\r\n    let dt = new Date();\r\n    let filename = \"Balance - \" + dt.getFullYear() + \"-\" + dt.getMonth() + \"-\" + dt.getDate();\r\n    // let { sheetName, fileName } =filename;\r\n    let targetTableElm = document.getElementById(tableId);\r\n    let wb = XLSX.utils.table_to_book(targetTableElm, <XLSX.Table2SheetOpts>{\r\n      sheet: \"Balance\"\r\n    });\r\n    XLSX.writeFile(wb, `${filename}.xlsx`);\r\n  }\r\n\r\n\r\n\r\n  /************************************************************************************************************************\r\n   *\r\n   * RESPONSE OK/ERROR\r\n   *\r\n   ************************************************************************************************************************/\r\n\r\n\r\n   responseOk(httpOperation:any, http: string, data:any, ws:any ){\r\n    switch(ws.enumUrl) {\r\n      case enumWS.BALANCE_MOVIMIENTO_CUENTA_TERCERO:\r\n        this.cuentaListado = this.cuentaSelected;\r\n        this.procesarData(data);\r\n\r\n      break;\r\n    }\r\n   }\r\n\r\n   responseError(urlResource: string,httpOperation: string, data: any, ws?: any){\r\n    alert(\"error al generar el balance\");\r\n   }\r\n}\r\n\r\n","<section id=\"collapse\">\r\n    <div class=\"row row-grid mt-2 mb-2\">\r\n        <div class=\"col-12 col-md-6 col-lg-6 col-xl-6 mt-1\">\r\n            <periodo-select [(fInicio)]=\"fechaInicio\" [(fFin)]=\"fechaFin\"></periodo-select>\r\n        </div>\r\n        <div class=\"col-12 col-md-6 col-lg-6 col-xl-6\">\r\n            <fieldset class=\"form-group div-cuenta\">\r\n                <label for=\"input-nombre\">Cuenta</label>\r\n                <cuenta-select (changeEvent)=\"selectCuenta($event)\"></cuenta-select>\r\n            </fieldset>\r\n        </div>\r\n        <div class=\"col-12 col-md-4 col-lg-6 col-xl-4\">\r\n            <!-- <div class=\"col-6 col-md-4 col-lg-3\"> -->\r\n            <fieldset class=\"form-group mr-2\" *ngIf=\"empresa.multipleMoneda == 1\">\r\n                <label for=\"input-nombre\">Moneda</label>\r\n                <sm-select [options]=\"selectTipoVistaOption\" [initValue]=\"initValueTipoVista\" (changeEvent)=\"tipoVistaChange($event)\"></sm-select>\r\n            </fieldset>\r\n            <!-- </div> -->\r\n        </div>\r\n\r\n    </div>\r\n    <div class=\"row mb-2\">\r\n        <div class=\"col-12 text-center\">\r\n            <button class=\"btn btnl-lg btn-primary mt-1\" (click)=\"refresh(true)\">GENERAR</button>\r\n        </div>\r\n    </div>\r\n    <div class=\"row\">\r\n        <div class=\"col-12 text-center\">\r\n            <exportar-data (clickExcel)=\"exportar()\"></exportar-data>\r\n        </div>\r\n\r\n    </div>\r\n\r\n    <ng-template [ngIf]=\"loaded\">\r\n        <h3>{{cuentaListado.nombre}}</h3>\r\n        <div class=\"container-table\">\r\n            <!-- Que contenga las columnas -->\r\n            <table width=\"100%\" class=\"table table-transparent table-striped table-dark\">\r\n                <thead>\r\n                    <tr>\r\n                        <th width=\"25%\"> <!-- TERCERO -->\r\n                            Tercero\r\n                        </th>\r\n                        <th width=\"8%\"> <!-- FECHA -->\r\n                            Fecha\r\n                        </th>\r\n                        <th width=\"21%\"> <!-- OBS -->\r\n                            Observaciones\r\n                        </th>\r\n                        <th width=\"10%\"> <!-- REFERENCIA -->\r\n                            Referencia\r\n                        </th>\r\n                        <th width=\"12%\"> <!-- DEBITO -->\r\n                            Debito\r\n                        </th>\r\n                        <th width=\"12%\"> <!-- CREDITO -->\r\n                            Credito\r\n                        </th>\r\n                        <th width=\"12%\"> <!-- SALDO -->\r\n                            Saldo\r\n                        </th>\r\n                    </tr>\r\n                