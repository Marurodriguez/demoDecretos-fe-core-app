        this.posAsiento = 0;
        this.fecha = new _ng_bootstrap_ng_bootstrap__WEBPACK_IMPORTED_MODULE_2__["NgbDate"](2020, 9, 26);
        this.fechaFin = new _ng_bootstrap_ng_bootstrap__WEBPACK_IMPORTED_MODULE_2__["NgbDate"](2022, 1, 1);
        this.page = 0;
        this.size = 10;
        this.tipoVista = TipoVistaEnum.original;
