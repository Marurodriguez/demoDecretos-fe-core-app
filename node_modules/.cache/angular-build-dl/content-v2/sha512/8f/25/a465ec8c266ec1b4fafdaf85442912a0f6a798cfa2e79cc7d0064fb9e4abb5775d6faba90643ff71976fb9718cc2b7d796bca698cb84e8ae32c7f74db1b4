**************************************************************************\r\n   * Variables\r\n   *\r\n   ************************************************************************************************************************/\r\n    public selectMonedaConfig: SmSelectOptions;\r\n    public initValue: MonedaModel;\r\n    public loaded: boolean = false; \r\n   /************************************************************************************************************************\r\n    * Constructor && Implements\r\n    *\r\n    ************************************************************************************************************************/\r\n   constructor(public dataService: DataService,\r\n               public cd: ChangeDetectorRef,\r\n               public userService: UserService, public mainService: MainService,\r\n               public functionService: FunctionService){\r\n    this.selectMonedaConfig = new SmSelectOptions();\r\n    this.selectMonedaConfig.bindLabel = \"nombre\";\r\n    this.selectMonedaConfig.bindValue = \"id\";\r\n    this.selectMonedaConfig.url = enumWS.MONEDA_PAGINATE;\r\n    this.selectMonedaConfig.searchable =false;\r\n    this.selectMonedaConfig.name = \"Moneda\";\r\n    this.selectMonedaConfig.items = this.mainService.monedas;\r\n\r\n    const paginateListModel = new PaginateListModel(0,100);\r\n    paginateListModel.addParameter(\"empresa\", this.userService.getIdEmpresaSelected().toString());\r\n   \r\n    // Cargar las monedas\r\n    // TODO: Tener un service central con los caches de las cosas que no cambian.\r\n    if(this.mainService.monedas.length > 0){\r\n      this.loaded = true;\r\n    }else{\r\n      this.dataService.httpFunction(enumWS.MONEDA_PAGINATE,this,paginateListModel,{});\r\n    }\r\n   }\r\n\r\n  /************************************************************************************************************************\r\n   * FUNCTIONS\r\n   *\r\n  ************************************************************************************************************************/\r\n   ngOnInit(){\r\n    this.selectInitValue();\r\n   } \r\n\r\n   selectInitValue(){\r\n    if(this.moneda){\r\n      this.initValue = this.moneda; \r\n    }else{\r\n      if(this.mainService.monedas.length>0){\r\n        this.initValue = this.mainService.monedas[0];\r\n      }\r\n    }\r\n   }\r\n\r\n   \r\n   getField(value:string): string {\r\n    try {\r\n      switch(value){\r\n        case \"moneda-color\":\r\n          if(this.moneda &&  this.moneda?.color) {\r\n            return this.moneda.color;\r\n          }else{\r\n            return \"normal\";\r\n          }\r\n        break;\r\n        case \"moneda-nombre\":\r\n          if(this.moneda && this.moneda?.nombre){\r\n            return this.moneda?.nombre;\r\n          }\r\n        break;\r\n        case \"moneda-codigo\":\r\n          if(this.moneda && this.moneda?.codigo){\r\n            return this.moneda?.codigo;\r\n          }\r\n      }\r\n      return \"\";\r\n    } catch (error) {\r\n      return \"\";\r\n    }\r\n   }\r\n\r\n  EmitChangeEvent(value: any){\r\n    this.changeEvent.emit(value);\r\n   }\r\n\r\n   selectMoneda(moneda: MonedaModel){\r\n    this.EmitChangeEvent(moneda);\r\n   }\r\n  /************************************************************************************************************************\r\n   *\r\n   * RESPONSE OK/ERROR\r\n   *\r\n   ************************************************************************************************************************/\r\n\r\n\r\n   responseOk(httpOperation:any, http: string, data:PaginateDataModel, ws:any ){\r\n    switch(ws.enumUrl) {\r\n      case enumWS.MONEDA_PAGINATE:\r\n        this.mainService.monedas = data.content; \r\n        this.selectMonedaConfig.items = this.mainService.monedas;\r\n        this.selectInitValue();\r\n        this.loaded = true;\r\n        this.cd.markForCheck();        \r\n      break;\r\n    }\r\n   }\r\n\r\n   responseError(urlResource: string,httpOperation: string, data: any, ws?: any){\r\n\r\n  }\r\n}\r\n","<ng-template [ngIf]=\"loaded\">\r\n    \r\n    <!-- Mostrar Select -->\r\n    <ng-template [ngIf]=\"tipo == 'select-large' || tipo == 'select-small'\">\r\n        <div class=\"div-moneda-select\" *ngIf=\"loaded\">\r\n            <sm-select class=\"select-moneda\" [options]=\"selectMonedaConfig\" [initValue]=\"initValue\" (changeEvent)=\"selectMoneda($event)\"> </sm-select>\r\n        </div>\r\n    </ng-template>\r\n\r\n    <!-- Mostrar Label-small -->\r\n    <ng-template [ngIf]=\"tipo == 'label-small'\">\r\n        <p class=\"cuenta color-{{getField('moneda-color')}}\" ngbTooltip=\"{{ getField('moneda-nombre')}}\">{{ getField('moneda-codigo') }}</p>\r\n    </ng-template>\r\n        <!-- Mostrar Label-large -->\r\n        <ng-template [ngIf]=\"tipo == 'label-large'\">\r\n            <p class=\"cuenta color-{{getField('moneda-color')}}\">{{ getField('moneda-nombre') }}</p>\r\n        </ng-template>\r\n</ng-template>\r\n","import { ChangeDetectorRef, Component, EventEmitter, Input, Output } from '@angular/core';\r\nimport { ActivatedRoute, Router, UrlSerializer } from '@angular/router';\r\nimport { AppConfig } from '../../../../../coreApp/src/app/app.config';\r\nimport { PaginateDataModel } from '../../../../../coreApp/src/app/models/PaginateData.model';\r\nimport { PaginateListModel } from '../../../../../coreApp/src/app/models/PaginateList.model';\r\nimport { DataService } from '../../../../../coreApp/src/app/services/data.services';\r\nimport { FunctionService } from '../../../../../coreApp/src/app/services/function.services';\r\nimport { AuthUserService } from '../../../../../coreApp/src/app/shared/auth/auth-user.service';\r\nimport { SmSelectOptions } from '../../../../../coreApp/src/app/shared/components/sm-select/sm-select.component';\r\nimport { CuentaContableModel } from '../../models/CuentaContable.model';\r\nimport { EmpresaModel } from '../../models/Empresa.model';\r\nimport { MonedaModel } from '../../models/Moneda.model';\r\nimport { NegocioModel } from '../../models/Negocio.model';\r\nimport { enumWS } from '../../navigation/ws/ws-routes.config';\r\nimport { UserService } from '../../services/User.service';\r\n\r\n\r\n@Component({\r\n  selector: 'money-input',\r\n  templateUrl: './money-input.component.html',\r\n  styleUrls: ['./money-input.component.scss']\r\n})\r\n\r\nexport class MoneyInputComponent {\r\n  /************************************************************************************************************************\r\n   * Inputs && Outputs && Properties\r\n   *\r\n   ************************************************************************************************************************/\r\n  public _value: any = 0;\r\n  public _formatMoney:boolean = false; // Si est true, el disabled = true y solo sirve para mostrar totales\r\n  @Input() monedaPrimary: MonedaModel;\r\n  @Input() disabled: boolean = false;\r\n\r\n  @Input() showMoneda: boolean = true;\r\n  @Input() label: string = \"\";\r\n  @Input() initValue: number; \r\n  @Input() simple: boolean = false; // SOLO PUEDE ACTIVARSE CON FORMAT MONEY\r\n  @Input() cantDecimales: number = 2;\r\n\r\n  // @Input() formatMoney(): boolean ; \r\n  get formatMoney(): boolean {\r\n    return this._formatMoney;\r\n  }\r\n  @Input() set formatMoney(value: boolean) {\r\n    this._formatMoney = value;\r\n    this.refreshValues();\r\n  }\r\n\r\n\r\n\r\n  @Output() changeEvent: EventEmitter<any> = new EventEmitter;\r\n  public valueStr: string = \"\";\r\n\r\n  get value(): any {\r\n    \r\n    if(this._value == undefined){\r\n      this._value = 0;\r\n    }\r\n\r\n    if(this.formatMoney == false){\r\n      return this._value;\r\n    }else{\r\n      return this.functionService.formatMoney(this._value,\" \");\r\n    }\r\n}\r\n  @Input() set value(nro: any) {\r\n    this._value = nro;\r\n    this.refreshValues();\r\n  } \r\n  @Output() valueChange = new EventEmitter<Number>();\r\n  /************