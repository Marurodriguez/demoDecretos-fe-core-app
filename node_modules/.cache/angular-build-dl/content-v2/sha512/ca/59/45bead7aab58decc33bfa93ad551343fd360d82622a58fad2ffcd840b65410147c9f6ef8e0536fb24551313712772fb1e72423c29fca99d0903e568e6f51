an = true;\r\n    public verMovimientosIntervalo: boolean = true;\r\n    public monedaVista: MonedaModel;\r\n    public tipoVista: number = 0; \r\n    public selectTipoVistaOption = new SmSelectOptions();\r\n    public initValueTipoVista: any;\r\n\r\n    public formatMoney: boolean = true;\r\n    public cantDecimales: number = 2;\r\n    public total: BalanceGrupoModel = new BalanceGrupoModel();\r\n    public tipoInforme: TipoInformeContableEnum = TipoInformeContableEnum.BALANCE_ESF;\r\n    public nivelSelectOption: SmSelectOptions = new SmSelectOptions();\r\n    public nivelVista: number = -1;\r\n    public initValueNivel: any;\r\n    /************************************************************************************************************************\r\n    * Constructor && Implements\r\n    *\r\n    ************************************************************************************************************************/\r\n   constructor(public dataService: DataService,\r\n               public appConfig: AppConfig, private cd: ChangeDetectorRef,\r\n               public router: Router, public route: ActivatedRoute,\r\n               public authUserService: AuthUserService, public serializer: UrlSerializer, public functionService: FunctionService,\r\n               public userService: UserService, public titleBarService: TitleBarService){\r\n    \r\n              this.empresa = this.userService.getEmpresaSelected();\r\n              this.monedaVista = this.empresa.monedaPrimaria;\r\n\r\n                        \r\n              this.selectTipoVistaOption.searchable =false;\r\n              this.selectTipoVistaOption.name =\"TipoVista-Select\";\r\n              this.selectTipoVistaOption.items = [\r\n                                                  {\"id\":TipoVistaEnum.principal, \"nombre\":this.empresa.monedaPrimaria.nombre},\r\n                                                  {\"id\":TipoVistaEnum.secundaria, \"nombre\":this.empresa.monedaSecundaria.nombre},\r\n                                                    ];              \r\n              this.initValueTipoVista = this.selectTipoVistaOption.items[0];\r\n\r\n              this.nivelSelectOption.searchable = false; this.nivelSelectOption.name = \"Nivel-Select\";\r\n              this.nivelSelectOption.items = [  {\"id\": -1, \"nombre\": \"Todos los niveles\"}, {\"id\": 0, \"nombre\": \"Solo Cuentas Principales\"}, {\"id\": 1, \"nombre\": \"Nivel 1 y Superior\"},{\"id\": 2, \"nombre\": \"Nivel 2 y Superior\"},{\"id\": 3, \"nombre\": \"Nivel 3 y Superior\"}];\r\n              this.initValueNivel = this.nivelSelectOption.items[0];\r\n  }\r\n\r\n   ngOnInit(){\r\n    // OJO QUE TOMA EL FECHAINICIO Y FECHA FIN SETEADO EN EL INICIO\r\n    // this.refresh(true); // Está la del tipoVistaChange\r\n\r\n    this.parametros = this.route.snapshot.data;\r\n    if(this.parametros?.tipoInforme){\r\n      this.tipoInforme = this.parametros?.tipoInforme;\r\n    }\r\n\r\n    switch(this.tipoInforme){\r\n      case TipoInformeContableEnum.BALANCE_ESF:\r\n        this.titleBarService.title = \"Balance ESF\";\r\n        this.titleBarService.subTitle = \"Desde esta pantalla se puede generar el balance por periodo\";\r\n      break;\r\n      case TipoInformeContableEnum.ESTADO_DE_RESULTADOS:\r\n        this.titleBarService.title = \"Estado de Resultados\";\r\n        this.titleBarService.subTitle = \"Desde esta pantalla se puede generar el balance por periodo\";\r\n      break;\r\n    }\r\n    \r\n\r\n   }\r\n   nivelChange(nivelSelected: any){\r\n    this.nivelVista = nivelSelected.id;\r\n    this.cd.markForCheck();\r\n   }\r\n   tipoVistaChange(TipoVistaSelected:any){\r\n    switch(TipoVistaSelected.id){\r\n      case TipoVistaEnum.principal: \r\n        this.tipoVista = TipoVistaEnum.principal;\r\n        this.monedaVista = this.empresa.monedaPrimaria;\r\n      break;\r\n      case TipoVistaEnum.secundaria:\r\n        this.tipoVista = TipoVistaEnum.secundaria;\r\n        this.monedaVista = this.empresa.monedaSecundaria;\r\n      break;\r\n    }\r\n    this.refresh(true);\r\n    //this.cd.markForCheck();\r\n   }\r\n\r\n\r\n  /************************************************************************************************************************\r\n   * Funciones\r\n   *\r\n   ************************************************************************************************************************/\r\n   getStyle(item: any):string {\r\n    try {\r\n        let tab: number = 25;\r\n        let regla: string = \"\";\r\n        tab = tab * item.nivel;\r\n        regla = \"text-indent: \" + tab.toString() + \"px\"; \r\n        return regla;\r\n    }catch(ex){\r\n      return \"\";\r\n    }\r\n    return \"\";\r\n   }\r\n   getField(item: any, fieldName: string): string {\r\n    try {\r\n      switch(fieldName){\r\n        case \"cuenta-nombre\":\r\n          // Devuelve 4 espacios por cada item\r\n          const space: string = \"    \";\r\n          let nombre: string = item.nombre;\r\n          if(item.nivel == 0){\r\n            nombre = nombre.toLocaleUpperCase();\r\n          }\r\n          let esp:string = \"\";\r\n          for(let x=1; x<=item.nivel;x++){\r\n              esp = esp + String(space);\r\n          }\r\n          let str = nombre;\r\n          return str;\r\n        break;\r\n      }\r\n    }catch(ex){\r\n      return \"\";\r\n    }\r\n    return \"\";\r\n   }\r\n\r\n   refresh(force:boolean){\r\n    let balance: BalanceRequestModel = new BalanceRequestModel();\r\n    balance.empresa = this.empresa.id;\r\n    balance.fechaInicio = this.functionService.getStringFromNgbDate(this.fechaInicio);\r\n    balance.fechaFin =  this.functionService.getStringFromNgbDate(this.fechaFin);\r\n    balance.tipoInforme = this.tipoInforme;\r\n\r\n    this.dataService.httpFunction(enumWS.BALANCE_GENERAR,this,balance,{});\r\n    // this.loaded = true; //HARDCODE\r\n    return true;\r\n   }\r\n\r\n   /**\r\n    * Indica Si se debe mostrar el registro\r\n    * @param BalanceGrupo\r\n    */\r\n   debeMostrarCuenta(item: BalanceGrupoModel){\r\n    \r\n    if(this.verSinMovimientos == false){\r\n      // Filtrar los de sin movimiento\r\n\r\n      // Si es jefe de un grupo borrarlo.. \r\n      //TODO: MEJORAR: lOS GRUPOS CON TODO CERO DEBERIAN BORRARSE TAMBIÉN\r\n      if(item.subCuentasList.length > 0 && item.nivel > 0 && item.ginicialCreditoMonedaPrimaria == 0\r\n        && item.ginicialDebitoMonedaPrimaria == 0 && item.gtotalDebitoMonedaPrimaria == 0 && item.gtotalCreditoMonedaPrimaria == 0 ){\r\n        return false;\r\n      }\r\n\r\n      if(item.totalDebitoMonedaPrimaria == 0 && item.totalCreditoMonedaPrimaria == 0 &&\r\n         item.inicialDebitoMonedaPrimaria == 0 && item.inicialCreditoMonedaPrimaria == 0 && item.subCuentasList.length == 0){\r\n          return false;\r\n      }\r\n    }\r\n    if (! (this.nivelVista == -1 || item.nivel <= this.nivelVista )){\r\n      return false;\r\n    }\r\n\r\n\r\n    return true;\r\n   }\r\n\r\n   getClass(item: BalanceGrupoModel):string{\r\n    let tipo: string = item.tipo.replace(\" \",\"-\");\r\n    return tipo;\r\n   }\r\n\r\n   getMonto(item: BalanceGrupoModel, inicial: string, debito: string): number{\r\n    let value: number = 0;\r\n    switch(inicial){\r\n      case 'inicial':\r\n        if(debito == 'debito'){\r\n          value = item.vInicialDebito;\r\n        }else if(debito == 'credito'){\r\n          value = item.vInicialCredito;\r\n        }else if(debito == 'saldo'){\r\n          value = item.vInicial;\r\n        }\r\n      break;\r\n      case 'total':\r\n        if(debito == 'debito'){\r\n          value = item.vTotalDebito;\r\n        }else if(debito == 'credito'){\r\n          value = item.vTotalCredito;\r\n        }else if(debito == 'saldo'){\r\n          //-\r\n        }\r\n        \r\n      break;\r\n      case 'saldo':\r\n        if(debito == 'debito'){\r\n          // str = item.vTotalDebitoStr;\r\n        }else if(debito == 'credito'){\r\n          // str = item.vTotalCreditoStr;\r\n        }else if(debito == 'saldo'){\r\n          value = item.vSaldo;\r\n        }\r\n      break;\r\n      // GRUPOS----------------------------------------------------------------------------------->\r\n      case 'ginicial':\r\n        if(debito == 'saldo'){\r\n          value = item.vgInicial;\r\n        }\r\n      break;\r\n      case 'gtotal':\r\n        if(debito == 'debito'){\r\n          value = item.vgTotalDebito;\r\n        }else if(debito == 'credito'){\r\n          value = item.vgTotalCredito;\r\n        }else if(debito == 'saldo'){\r\n          //-\r\n        }\r\n      break;\r\n      case 'gsaldo':\r\n        if(debito == 'saldo'){\r\n          value = item.vgSaldo;\r\n        }\r\n      break;\r\n    }\r\n\r\n    return value;\r\n   }\r\n\r\n   getMontoStr(item: BalanceGrupoModel, inicial: string, debito: string): string{\r\n    let str: string = \"\";\r\n    switch(inicial){\r\n      case 'inicial':\r\n        if(debito == 'debito'){\r\n          str = item.vInicialDebitoStr;\r\n        }else if(debito == 'credito'){\r\n          str = item.vInicialCreditoStr;\r\n        }else if(debito == 'saldo'){\r\n          str = item.vInicialStr;\r\n        }\r\n      break;\r\n      case 'total':\r\n        if(debito == 'debito'){\r\n          str = item.vTotalDebitoStr;\r\n        }else if(debito == 'credito'){\r\n          str = item.vTotalCreditoStr;\r\n        }else if(debito == 'saldo'){\r\n          //-\r\n        }\r\n        \r\n      break;\r\n      case 'saldo':\r\n        if(debito == 'debito'){\r\n          // str = item.vTotalDebitoStr;\r\n        }else if(debito == 'credito'){\r\n          // str = item.vTotalCreditoStr;\r\n        }else if(debito == 'saldo'){\r\n          str = item.vSaldoStr;\r\n        }\r\n      break;\r\n      // GRUPOS----------------------------------------------------------------------------------->\r\n      case 'ginicial':\r\n        if(debito == 'saldo'){\r\n          str = item.vgInicialStr;\r\n        }\r\n      break;\r\n      case 'gtotal':\r\n        if(debito == 'debito'){\r\n          str = item.vgTotalDebitoStr;\r\n        }else if(debito == 'credito'){\r\n          str = item.vgTotalCreditoStr;\r\n        }else if(debito == 'saldo'){\r\n          //-\r\n        }\r\n      break;\r\n      case 'gsaldo':\r\n        if(debito == 'saldo'){\r\n          str = item.vgSaldoStr;\r\n        }\r\n      break;\r\n    }\r\n\r\n    return str;\r\n   }\r\n\r\n\r\n   addItemsToArray(array:BalanceGrupoModel[]){\r\n    if(array == undefined){\r\n      return new BalanceGrupoModel();\r\n    }\r\n    for(let x of array){\r\n      let reg: BalanceGrupoModel = new BalanceGrupoModel();\r\n      //reg = x;\r\n      //OJO QUE NO COPIA ANIDADOS!!!!!!\r\n      reg = this.functionService.copyValuesObject(reg,x);\r\n      this.addItemsToSubArray(reg,x.subCuentasList);\r\n      //reg.subCuentasList = x.subCuentasList;\r\n      \r\n      this.registros.push(reg);\r\n    }\r\n     \r\n   }\r\n   addItemsToSubArray(reg: BalanceGrupoModel, array:BalanceGrupoModel[]): BalanceGrupoModel{\r\n    for(let x of array){\r\n      let reg: BalanceGrupoModel = new BalanceGrupoModel();\r\n      reg = this.functionService.copyValuesObject(reg,x);\r\n      reg.subCuentasList = []\r\n      reg.subCuentasList.push(this.addItemsToSubArray(reg,x.subCuentasList));\r\n    }\r\n    return reg; \r\n   }\r\n\r\n   updateRegistros(array: BalanceGrupoModel[] ) {\r\n    \r\n    for(let reg of array){\r\n      this.updateRegistro(reg);\r\n      this.updateRegistros (reg.subCuentasList);\r\n    }\r\n   }\r\n   updateRegistro(reg: BalanceGrupoModel){\r\n    let inicialDebito: number = 0; let inicialCredito: number = 0;\r\n    let totalDebito: number = 0; let totalCredito: number = 0;\r\n    let gInicialDebito: number=0;let gInicialCredito: number =0;\r\n    let gTotalDebito: number = 0; let gTotalCredito: number = 0;\r\n\r\n\r\n    switch(this.tipoVista){\r\n      case TipoVistaEnum.principal: // Moneda Primaria\r\n        inicialDebito = reg.inicialDebitoMonedaPrimaria;\r\n        inicialCredito = reg.inicialCreditoMonedaPrimaria;\r\n        \r\n        totalDebito = reg.totalDebitoMonedaPrimaria;\r\n        totalCredito = reg.totalCreditoMonedaPrimaria;\r\n        \r\n        gInicialDebito = reg.ginicialDebitoMonedaPrimaria;\r\n        gInicialCredito = reg.ginicialCreditoMonedaPrimaria;\r\n        gTotalDebito = reg.gtotalDebitoMonedaPrimaria;\r\n        gTotalCredito = reg.gtotalCreditoMonedaPrimaria;\r\n      break;\r\n      case TipoVistaEnum.secundaria: // Moneda Secundaria\r\n        inicialDebito = reg.inicialDebitoMonedaSecundaria;\r\n        inicialCredito = reg.inicialCreditoMonedaSecundaria;\r\n        \r\n        totalDebito = reg.totalDebitoMonedaSecundaria;\r\n        totalCredito = reg.totalCreditoMonedaSecundaria;          \r\n\r\n        gInicialDebito = reg.ginicialDebitoMonedaSecundaria;\r\n        gInicialCredito = reg.ginicialCreditoMonedaSecundaria;\r\n       \r\n        gTotalDebito = reg.gtotalDebitoMonedaSecundaria;\r\n        gTotalCredito = reg.gtotalCreditoMonedaSecundaria;\r\n\r\n      break;\r\n    }\r\n      // Inicial\r\n      reg.vInicialDebito = inicialDebito;\r\n      reg.vInicialCredito = inicialCredito;\r\n      reg.vInicial = reg.vInicialDebito - reg.vInicialCredito\r\n      reg.vInicialStr = this.functionService.formatMoney(reg.vInicial,this.monedaVista.codigo);\r\n      reg.vInicialDebitoStr = this.functionService.formatMoney(reg.vInicialDebito,this.monedaVista.codigo);\r\n      reg.vInicialCreditoStr = this.functionService.formatMoney(reg.vInicialCredito,this.monedaVista.codigo);\r\n\r\n      // Total\r\n      reg.vTotalDebito = totalDebito;\r\n      reg.vTotalCredito = totalCredito;\r\n      reg.vTotal = reg.vTotalDebito - reg.vTotalCredito\r\n      reg.vTotalDebitoStr = this.functionService.formatMoney(reg.vTotalDebito,this.monedaVista.codigo);\r\n      reg.vTotalCreditoStr = this.functionService.formatMoney(reg.vTotalCredito,this.monedaVista.codigo);\r\n\r\n      // Saldo\r\n      reg.vSaldoDebito = reg.vInicialDebito + reg.vTotalDebito;\r\n      reg.vSaldoCredito = reg.vInicialCredito + reg.vTotalCredito;\r\n      reg.vSaldo = reg.vSaldoDebito - reg.vSaldoCredito\r\n      reg.vSaldoStr = this.functionService.formatMoney(reg.vSaldo,this.monedaVista.codigo);\r\n\r\n      // Grupo\r\n      if(gInicialDebito != 0 || gInicialCredito != 0 || gTotalDebito != 0 || gTotalCredito != 0){\r\n        reg.vTieneOperacionesGrupo = true;\r\n      }else{\r\n        reg.vTieneOperacionesGrupo = false;\r\n      }\r\n      reg.vgInicial =  gInicialDebito - gInicialCredito;\r\n      reg.vgInicialStr = this.functionService.formatMoney(reg.vgInicial,this.monedaVista.codigo);\r\n      \r\n      reg.vgTotalDebito = gTotalDebito;\r\n      reg.vgTotalCredito = gTotalCredito;\r\n      reg.vgTotalDebitoStr = this.functionService.formatMoney(reg.vgTotalDebito,this.monedaVista.codigo);\r\n      reg.vgTotalCreditoStr = this.functionService.formatMoney(reg.vgTotalCredito,this.monedaVista.codigo);\r\n      reg.vgTotal = reg.vgTotalDebito - reg.vgTotalCredito;\r\n      reg.vgTotalStr = this.functionService.formatMoney(reg.vgTotal,this.monedaVista.codigo);\r\n      reg.vgSaldo = reg.vgInicial + reg.vgTotal;\r\n      reg.vgSaldoStr = this.functionService.formatMoney(reg.vgSaldo,this.monedaVista.codigo);\r\n      \r\n\r\n   }\r\n\r\n   /**\r\n    * Suma las cuentas de registros!\r\n    * @param tipo Indica el tipo de Cuenta: activo, pasivo, patrimonio neto\r\n    */\r\n   sumarCuentas(tipo: string): BalanceGrupoModel{\r\n    let suma: BalanceGrupoModel = new BalanceGrupoModel();\r\n    for (let reg of this.registros){\r\n      if(reg.tipo == tipo){\r\n        suma.vInicial += reg.vInicial;\r\n        suma.vTotalDebito += reg.vTotalDebito;\r\n        suma.vTotalCredito+= reg.vTotalCredito;\r\n        suma.vSaldo += reg.vSaldo;\r\n\r\n        suma.vgInicial += reg.vgInicial;\r\n        suma.vgTotalDebito += reg.vgTotalDebito;\r\n        suma.vgTotalCredito+=reg.vgTotalCredito;\r\n        suma.vgSaldo += reg.vgSaldo;\r\n      }\r\n    }\r\n    return suma; \r\n   }\r\n   \r\n\r\n   procesarData(data): boolean {\r\n    this.data = data;\r\n    this.registros = [];\r\n    let reg1: BalanceGrupoModel = new BalanceGrupoModel();\r\n    let reg2: BalanceGrupoModel = new BalanceGrupoModel();\r\n    this.total = new BalanceGrupoModel(); //RESET TOTAL\r\n\r\n    switch(this.tipoInforme){\r\n      case TipoInformeContableEnum.BALANCE_ESF:\r\n        this.addItemsToArray(data.activoList);\r\n        this.addItemsToArray(data.pasivoList);\r\n        this.addItemsToArray(data.patrimonioList);      \r\n        this.updateRegistros(this.registros);\r\n        reg1 = this.sumarCuentas(\"pasivo\");\r\n        reg2= this.sumarCuentas(\"patrimonio\");\r\n        this.total.vInicial = reg1.vgInicial + reg2.vgInicial;\r\n        this.total.vTotalDebito = reg1.vgTotalDebito + reg2.vgTotalDebito;\r\n        this.total.vTotalCredito = reg1.vgTotalCredito + reg2.vgTotalCredito;\r\n        this.total.vSaldo = this.total.vTotalDebito  + this.total.vTotalCredito; \r\n        this.total.nombre = \"Pasivo + Patrimonio\";\r\n\r\n      break;\r\n      case TipoInformeContableEnum.ESTADO_DE_RESULTADOS:\r\n        this.addItemsToArray(data.resultadoPositivoList);\r\n        this.addItemsToArray(data.resultadoNegativoList);\r\n        this.updateRegistros(this.registros);\r\n        reg1 = this.sumarCuentas(\"resultado positivo\");\r\n        reg2 = this.sumarCuentas(\"resultado negativo\");\r\n        this.total.vInicial = reg1.vgInicial - reg2.vgInicial;\r\n        this.total.vTotalDebito = reg1.vgTotalDebito - reg2.vgTotalDebito;\r\n        this.total.vTotalCredito = reg1.vgTotalCredito - reg2.vgTotalCredito;\r\n        this.total.vSaldo = this.total.vTotalDebito - this.total.vTotalCredito;        \r\n        this.total.nombre = \"Utilidad del Ejercicio (Resultado Pos - Resultado Neg)\";\r\n\r\n      break;\r\n    }\r\n    this.total.vTotalDebitoStr = this.functionService.formatMoney(this.total.vTotalDebito,this.monedaVista.codigo);\r\n    this.total.vTotalCreditoStr = this.functionService.formatMoney(this.total.vTotalCredito,this.monedaVista.codigo);\r\n    this.total.vSaldoStr = this.functionService.formatMoney(this.total.vSaldo,this.monedaVista.codigo);\r\n    this.total.vgInicialStr = this.functionService.formatMoney(this.total.vgInicial,this.monedaVista.codigo);\r\n    this.total.vInicialStr = this.functionService.formatMoney(this.total.vInicial,this.monedaVista.codigo);\r\n    this.total.vgTotalDebitoStr = this.functionService.formatMoney(this.total.vgTotalDebito,this.monedaVista.codigo);\r\n    this.total.vgTotalCreditoStr = this.functionService.formatMoney(this.total.vgTotalCredito,this.monedaVista.codigo);\r\n    this.total.vgSaldoStr = this.functionService.formatMoney(this.total.vgSaldo,this.monedaVista.codigo);\r\n\r\n    this.total.codigo = \"\";\r\n    this.total.idMoneda = reg2.idMoneda;\r\n    this.total.vTieneOperacionesGrupo = false;\r\n    this.total.subCuentasList = []; \r\n    this.total.nivel = -1;\r\n\r\n\r\n\r\n\r\n    this.registros.push(this.total);\r\n\r\n    this.loaded = true;\r\n    this.cd.markForCheck();\r\n    return true;\r\n   }\r\n\r\n   public exportar(): boolean {\r\n    this.formatMoney = false;\r\n    this.cd.markForCheck();\r\n    this.exportTableToExcel(\"tableData\");\r\n    //this.formatMoney =true;\r\n    return true;\r\n   }\r\n\r\n\r\n   exportTableToExcel(tableId: string, name?: string) {\r\n    let dt = new Date();\r\n    let filename = \"Balance - \" + dt.getFullYear() + \"-\" + dt.getMonth() + \"-\" + dt.getDate();\r\n    // let { sheetName, fileName } =filename;\r\n    let targetTableElm = document.getElementById(tableId);\r\n    let wb = XLSX.utils.table_to_book(targetTableElm, <XLSX.Table2SheetOpts>{\r\n      sheet: \"Balance\"\r\n    });\r\n    XLSX.writeFile(wb, `${filename}.xlsx`);\r\n  }\r\n\r\n  // exportArrayToExcel(arr: any[], name?: string) {\r\n  //   let { sheetName, fileName } = getFileName(name);\r\n\r\n  //   var wb = XLSX.utils.book_new();\r\n  //   var ws = XLSX.utils.json_to_sheet(arr);\r\n  //   XLSX.utils.book_append_sheet(wb, ws, sheetName);\r\n  //   XLSX.writeFile(wb, `${fileName}.xlsx`);\r\n  // }\r\n\r\n\r\n  /************************************************************************************************************************\r\n   *\r\n   * RESPONSE OK/ERROR\r\n   *\r\n   ************************************************************************************************************************/\r\n\r\n\r\n   responseOk(httpOperation:any, http: string, data:any, ws:any ){\r\n    switch(ws.enumUrl) {\r\n      case enumWS.BALANCE_GENERAR:\r\n        \r\n        this.procesarData(data);\r\n\r\n      break;\r\n    }\r\n   }\r\n\r\n   responseError(urlResource: string,httpOperation: string, data: any, ws?: any){\r\n    alert(\"error al generar el balance\");\r\n   }\r\n}\r\n\r\n","<section id=\"collapse\">\r\n    <div class=\"row row-grid mt-2 mb-2\">\r\n        <div class=\"col-12 col-md-6 col-lg-5 col-xl-4 mt-1\">\r\n            <periodo-select [(fInicio)]=\"fechaInicio\" [(fFin)]=\"fechaFin\"></periodo-select>\r\n        </div>\r\n\r\n        <div class=\"col-12 col-md-6 col-lg-4 col-xl-4\">\r\n            <!-- <div class=\"col-6 col-md-4 col-lg-3\"> -->\r\n            <fieldset class=\"form-group mr-2 form-group-label fieldset-moneda\" *ngIf=\"empresa.multipleMoneda == 1\">\r\n                <label for=\"input-nombre\">Moneda</label>\r\n                <sm-select [options]=\"selectTipoVistaOption\" [initValue]=\"initValueTipoVista\" (changeEvent)=\"tipoVistaChange($event)\"></sm-select>\r\n            </fieldset>\r\n            <!-- </div> -->\r\n        </div>\r\n        <div class=\"col-6 col-md-6 col-lg-3 col-xl-4 text-right\">\r\n            <div class=\"filtros text-left\">\r\n                <div class=\"custom-control custom-switch\">\r\